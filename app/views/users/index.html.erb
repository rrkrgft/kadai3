<h1>ユーザー一覧</h1>

<div class="mb-3">
  <table class="table table-hover">
    <tr>
      <th>ユーザーID</th>
      <th>ユーザー名</th>
    </tr>
    <% @users.each do |u| %>
    <tr>
      <th><%= link_to u.id, user_path(u.id) %></th>
      <th><%= link_to u.name, user_path(u.id) %></th>
    </tr>
    <% end %>
  </table>
</div>